<?xml version="1.0"?>
<Container version="2">
  <Name>immich-server</Name>
  <Repository>ghcr.io/immich-app/immich-server:release</Repository>
  <Network>bridge</Network>
  <WebUI>http://[IP]:[PORT:2283]</WebUI>
  <Category></Category>
  <Overview></Overview>
  <Description></Description>
  <Author></Author>
  <TemplateURL>https://raw.githubusercontent.com/username/repo/main/unraid/immich-server.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/username/repo/main/unraid/immich-server-logo.png</Icon>
  <Image></Image>
  <Config Name="WebUI" Target="2283" Default="2283" Mode="tcp" Description="WebUI Port" Type="Port" Display="always" Required="true" Mask="false">2283</Config>
  <Config Name="Volume for /usr/src/app/upload" Target="/usr/src/app/upload" Default="/temp" Mode="rw" Description="Default /temp" Type="Path" Display="advanced" Required="true" Mask="false">/temp</Config>
  <Config Name="Volume for /etc/localtime" Target="/etc/localtime" Default="/etc/localtime" Mode="rw" Description="Default /etc/localtime" Type="Path" Display="advanced" Required="true" Mask="false">/etc/localtime</Config>
</Container>